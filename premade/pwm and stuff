var TAU = 2 * Math.PI;

function dsp(t) {
  var lfo1 = Math.sin(TAU * t * P0 * 10);
  var lfo2 = Math.sin(TAU * t * P0 * 0.5);
  return 0.5 * (0.5 + 0.25 * lfo1) * (P1 * sq(t, 440, lfo2) + P2 * sq(t, 1760, 1 - (lfo2 * 0.3) * P3));

}

function sin(t, f) {
  return Math.sin(TAU * t * f);
}



function sq(t, f, pw) {
  return Math.sin(t * TAU * f) > pw ? 1 : -1;
}

